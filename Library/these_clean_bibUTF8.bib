  ISSN     = {1089778X},
  JOURNAL  = {IEEE Transactions on Evolutionary Computation},
  MONTH    = {04},
  NUMBER   = {2},
  PAGES    = {182--197},
  TITLE    = {{A fast and elitist multiobjective genetic algorithm: NSGA-II}},
  URL      = {http://ieeexplore.ieee.org/xpl/login.jsp?tp=&arnumber=996017&url=http://ieeexplore.ieee.org/xpls/abs_all.jsp?arnumber=996017},
  VOLUME   = {6},
