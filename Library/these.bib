@techreport{Horvitz1971,
abstract = {POPSIM, a dynamic demographic model designed for computer simulation of the principal demographic processes occurring in human populations is described. Since the model generates a vital event history, including the dates of birth, marriage, divorce or widowhood, remarriage, and death for each individual in the computer population, it is classed as a microsimulation model. It is a 2-sex model and may be used for simulating either cohort or period data. POPSIM is a stochastic, rather than a deterministic model in that random sampling of probability distributions is used to determine which events occur to each individual and when they occur. It is a dynamic model since it permits the probabilities to change with time. The model can be made self-adjusting through the use of feedback mechanisms. For example, feedback models to account for the effect of changing marriage patterns on marriage rates have been developed. - See more at:},
author = {Horvitz, Daniel G. and Giesbrecht, F. G. and Shah, B. V. and Lachenbruch, P. A.},
institution = {Carolina Population Center},
pages = {45--70},
title = {{POPSIM, a demographic microsimulation model}},
year = {1971}
}
