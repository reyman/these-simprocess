@article{Blum2003,
abstract = {The field of metaheuristics for the application to combinatorial optimization problems is $\backslash$na rapidly growing field of research. This is due to the importance of combinatorial $\backslash$noptimization problems for the scientific as well as the industrial world.We give a survey $\backslash$nof the nowadays most important metaheuristics from a conceptual point of view.We $\backslash$noutline the different components and concepts that are used in the different $\backslash$nmetaheuristics in order to analyze their similarities and differences. Two very $\backslash$nimportant concepts in metaheuristics are intensification and diversification. These are $\backslash$nthe two forces that largely determine the behavior of a metaheuristic. They are in some $\backslash$nway contrary but also complementary to each other.We introduce a framework, that we $\backslash$ncall the I\&D frame, in order to put different intensification and diversification $\backslash$ncomponents into relation with each other. Outlining the advantages and disadvantages $\backslash$nof different metaheuristic approaches we conclude by pointing out the importance of $\backslash$nhybridization of metaheuristics as well as the integration of metaheuristics and other $\backslash$nmethods for optimization.},
author = {Blum, Christian and Roli, Andrea},
doi = {10.1145/937503.937505},
isbn = {0360-0300},
issn = {0360-0300},
journal = {ACM Computing Surveys},
